package pruebas;

public class Lista {
	Nodo inicio;
	Nodo fin;
	
	public Lista(){
		inicio=null;
		fin=null;
	}
	
	public void insertarInicio(String info){
		Nodo nuevo=new Nodo(info, inicio);
		inicio=nuevo;
		if(fin==null)
			fin=inicio;
	}
	public void insertarFinal(String info){
		Nodo nuevo=new Nodo(info, null);
		if(inicio ==null){
			fin=nuevo;
			inicio=fin;
		}else{
			fin.siguiente=nuevo;
			fin=nuevo;
		}
		
	}
	
	public void eliminarInicio(){
		inicio=inicio.siguiente;
	}
	
	public void Listar(){
		Nodo temp=inicio;
		while(temp != null){
			System.out.println(temp.getInfo());
			temp=temp.siguiente;
		}
	}

}
